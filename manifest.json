{
"name"             : "Go To Work",
"version"          : "0.0.1",
"manifest_version" : 2,
"description"      : "Make you more efficient in Study or Work",
"icons"            : {
	"19"           : "icon/icon19.png",
	"48"		   : "icon/icon48.png",
	"128"		   : "icon/icon128.png"
	},
"browser_action"   : {
    "default_icon"  : "icon/icon19.png",
    "default_popup" : "popup.html"
    },

"permissions"    : [
	"tabs"
    ],
"background"     :{
	"scripts"    : ["js/blocks.js"]
	}
}